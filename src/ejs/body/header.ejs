        <span class="withIcon accent darkhover ic-menu">Menu</span></label
      >
    </li>
    <li id="tab-header">
      <a href="#" class="nostyle">
        <span class="withIcon accent darkhover ic-me">Jamie Lievesley</span></a
      >
    </li>
    <li id="tab-profiles">
      <a href="#profiles" class="nostyle">
        <span class="withIcon accent darkhover ic-people">Profiles</span></a
      >
    </li>
    <li id="tab-portfolio">
      <a href="#portfolio" class="nostyle">
        <span class="withIcon accent darkhover ic-briefcase">Portfolio</span></a
      >
    </li>
    <li id="tab-about">
      <a href="#about" class="nostyle">
        <span class="withIcon accent darkhover ic-information"
          >About&nbsp;Me</span
        ></a
      >
    </li>
    <li id="tab-contact">
      <a href="#contact" class="nostyle">
        <span class="withIcon accent darkhover ic-email">Contact</span></a
      >
    </li>
  </ul>
  <label for="nav-control-cb" id="nav-control-inside"></label>
