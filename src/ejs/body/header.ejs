<!-- Large header with name, logo, parallax scrolling background. -->
<header>
  <div id="container-header-brand">
    <h1 itemprop="name">Jamie Lievesley</h1>
    <img src="./img/logo_circle_64.png" />
  </div>
</header>

<!-- Fragment navigation; as tabs on desktop, drop-down menu on mobile. -->
<input type="checkbox" id="nav-control-cb" class="hidden" />
<label for="nav-control-cb" id="nav-control-outside"></label>
<nav>
  <ul>
